{
  "expo": {
    "name": "Mood Maps",
    "slug": "mood-maps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mood-maps-icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#F0F4F8",
    "splash": {
      "image": "./assets/mood-maps-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moodmaps.app",
      "userInterfaceStyle": "light"
    },
    "android": {
      "package": "com.moodmaps.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/mood-maps-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "b62bb1f0-70be-404e-9507-6b24f826e628"
      }
    }
  }
}
